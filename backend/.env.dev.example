# Environment configuration for DEVELOPMENT
APP_ENV=development

# Database configuration for development (SQLite)
DB_DRIVER=sqlite
DB_PATH=database/webapp.db

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-32-chars-min
JWT_EXPIRY=3600

# Advertising Configuration
AD_PROVIDER_ID=ca-pub-1234567890123456
AD_SLOT_ID=1234567890
AD_ENABLED=false

# Multi-Langue Selector
LANG_DEFAULT=fr
LANG_SELECTOR=false
